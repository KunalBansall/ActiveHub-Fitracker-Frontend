    "@types/react-datepicker": "^7.0.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^11.13.5",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.3.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.4.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.28.0",
